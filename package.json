{
  "name": "styled-kit",
  "version": "0.3.1",
  "description": "A components kit for React based on styled-components",
  "license": "UNLICENSED",
  "repository": {
    "type": "git",
    "url": "https://robert_kirsz@bitbucket.org/robert_kirsz/styled-kit.git"
  },
  "author": {
    "name": "Robert Kirsz",
    "email": "robert.kirsz@gmail.com"
  },
  "scripts": {
    "start": "NODE_ENV=dev webpack-dev-server --config ./webpack.dev.config.js --hot",
    "build": "NODE_ENV=prod webpack --config ./webpack.prod.config.js",
    "test": "NODE_ENV=test jest --watch",
    "test:verbose": "NODE_ENV=test jest --verbose --watch",
    "test:coverage": "NODE_ENV=test jest --coverage",
    "lint": "eslint src",
    "lint:fix": "eslint --fix src",
    "prettier": "prettier './src/{**/*.js, *.js}'",
    "prettier:fix": "prettier './src/{**/*.js, *.js}' --write"
  },
  "dependencies": {
    "react": "16.6.3",
    "react-dom": "16.6.3",
    "styled-components": "^4.1.1"
  },
  "devDependencies": {
    "@babel/core": "7.1.6",
    "@babel/plugin-proposal-class-properties": "7.1.0",
    "@babel/plugin-proposal-export-default-from": "7.0.0",
    "@babel/plugin-proposal-optional-chaining": "7.0.0",
    "@babel/preset-env": "7.1.6",
    "@babel/preset-react": "7.0.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "10.0.1",
    "babel-jest": "23.6.0",
    "babel-loader": "8.0.4",
    "babel-plugin-styled-components": "1.9.0",
    "eslint": "5.9.0",
    "eslint-config-standard": "12.0.0",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-jsx-a11y": "6.1.2",
    "eslint-plugin-node": "8.0.0",
    "eslint-plugin-promise": "4.0.1",
    "eslint-plugin-react": "7.11.1",
    "eslint-plugin-standard": "4.0.0",
    "html-webpack-plugin": "3.2.0",
    "jest": "23.6.0",
    "jest-dom": "2.1.1",
    "prettier": "1.15.2",
    "react-testing-library": "5.3.0",
    "webpack": "4.26.1",
    "webpack-cli": "3.1.2",
    "webpack-dev-server": "3.1.10"
  },
  "peerDependencies": {
    "react": ">= 16.6.3",
    "react-dom": ">= 16.6.3",
    "styled-components": ">= 4.1.1"
  }
}
