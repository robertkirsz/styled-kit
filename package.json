{
  "name": "styled-kit",
  "version": "1.0.0",
  "description": "A React UI components kit powered by styled-components",
  "author": "Robert Kirsz <robert.kirsz@gmail.com> (https://robertkirsz.com/)",
  "license": "MIT",
  "homepage": "http://robertkirsz.github.io/styled-kit",
  "repository": {
    "type": "git",
    "url": "https://github.com/robertkirsz/styled-kit.git"
  },
  "keywords": [
    "react",
    "styled-components",
    "ui"
  ],
  "scripts": {
    "start": "NODE_ENV=dev webpack-dev-server --config webpack.config.dev.js --hot",
    "build": "NODE_ENV=prod webpack --config webpack.config.prod.js",
    "build-demo": "NODE_ENV=prod webpack --config webpack.config.dev.js",
    "test": "NODE_ENV=test jest --watch",
    "test:verbose": "NODE_ENV=test jest --verbose --watch",
    "test:coverage": "NODE_ENV=test jest --coverage",
    "lint": "eslint './src/{**/*.js,*.js}' --fix",
    "format": "prettier './src/{**/*.js,*.js}' --write --check",
    "predeploy": "npm run build-demo",
    "deploy": "gh-pages -d demo"
  },
  "dependencies": {
    "@babel/runtime": "^7.6.3",
    "react": "^16.10.2",
    "react-dom": "^16.10.2",
    "styled-components": "^4.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.6.4",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-export-default-from": "^7.5.2",
    "@babel/plugin-proposal-optional-chaining": "^7.6.0",
    "@babel/plugin-transform-runtime": "^7.6.2",
    "@babel/preset-env": "^7.6.3",
    "@babel/preset-react": "^7.6.3",
    "@testing-library/jest-dom": "^4.1.2",
    "@testing-library/react": "^9.3.0",
    "@typescript-eslint/eslint-plugin": "^2.3.3",
    "@typescript-eslint/parser": "^2.3.3",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-styled-components": "^1.10.6",
    "eslint": "^6.5.1",
    "eslint-config-react-app": "^5.0.2",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-flowtype": "^3.13.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.16.0",
    "eslint-plugin-react-hooks": "^1.7.0",
    "gh-pages": "^2.1.1",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^24.9.0",
    "jest-dom": "^4.0.0",
    "jest-styled-components": "^6.3.3",
    "prettier": "^1.18.2",
    "webpack": "^4.41.1",
    "webpack-cli": "^3.3.9",
    "webpack-dev-server": "^3.8.2"
  },
  "peerDependencies": {
    "react": "^16.10.2",
    "react-dom": "^16.10.2",
    "styled-components": "^4.4.0"
  }
}
