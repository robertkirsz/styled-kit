{
  "name": "styled-kit",
  "version": "1.0.2",
  "description": "A React UI components kit powered by styled-components",
  "author": "Robert Kirsz <robert.kirsz@gmail.com>",
  "license": "MIT",
  "homepage": "http://robertkirsz.github.io/styled-kit",
  "repository": {
    "type": "git",
    "url": "https://github.com/robertkirsz/styled-kit.git"
  },
  "scripts": {
    "start": "NODE_ENV=dev webpack-dev-server --config webpack.config.dev.js --hot",
    "build": "NODE_ENV=prod webpack --config webpack.config.prod.js",
    "build-demo": "NODE_ENV=prod webpack --config webpack.config.dev.js",
    "test": "NODE_ENV=test jest --watch",
    "test:verbose": "NODE_ENV=test jest --verbose --watch",
    "test:coverage": "NODE_ENV=test jest --coverage",
    "lint": "eslint './src/{**/*.js,*.js}' --fix",
    "format": "prettier './src/{**/*.js,*.js}' --write --check",
    "predeploy": "npm run build-demo",
    "deploy": "gh-pages -d demo"
  },
  "devDependencies": {
    "@babel/core": "7.9.0",
    "@babel/plugin-proposal-class-properties": "7.8.3",
    "@babel/plugin-proposal-export-default-from": "7.8.3",
    "@babel/plugin-proposal-optional-chaining": "7.9.0",
    "@babel/plugin-transform-runtime": "7.9.0",
    "@babel/preset-env": "7.9.0",
    "@babel/preset-react": "7.9.4",
    "@babel/runtime": "7.9.2",
    "@typescript-eslint/eslint-plugin": "2.25.0",
    "@typescript-eslint/parser": "2.25.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "10.1.0",
    "babel-jest": "25.1.0",
    "babel-loader": "8.1.0",
    "babel-plugin-styled-components": "1.10.7",
    "eslint": "6.8.0",
    "eslint-config-react-app": "5.2.1",
    "eslint-plugin-babel": "5.3.0",
    "eslint-plugin-flowtype": "4.7.0",
    "eslint-plugin-import": "2.20.1",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-react": "7.19.0",
    "eslint-plugin-react-hooks": "2.5.1",
    "gh-pages": "2.2.0",
    "html-webpack-plugin": "4.0.1",
    "jest": "25.1.0",
    "jest-styled-components": "7.0.0",
    "prettier": "2.0.2",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-test-renderer": "16.13.1",
    "styled-components": "5.0.1",
    "typescript": "3.8.3",
    "webpack": "4.42.1",
    "webpack-cli": "3.3.11",
    "webpack-dev-server": "3.10.3"
  }
}
