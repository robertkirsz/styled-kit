{
  "name": "styled-kit",
  "version": "0.4.0",
  "description": "A components kit for React based on styled-components",
  "license": "UNLICENSED",
  "repository": {
    "type": "git",
    "url": "https://robert_kirsz@bitbucket.org/robert_kirsz/styled-kit.git"
  },
  "author": {
    "name": "Robert Kirsz",
    "email": "robert.kirsz@gmail.com"
  },
  "scripts": {
    "start": "NODE_ENV=dev webpack-dev-server --config ./webpack.config.dev.js --hot",
    "build": "NODE_ENV=prod webpack --config ./webpack.config.prod.js",
    "test": "NODE_ENV=test jest --watch",
    "test:verbose": "NODE_ENV=test jest --verbose --watch",
    "test:coverage": "NODE_ENV=test jest --coverage",
    "lint": "eslint './src/{**/*.js, *.js}'",
    "lint:fix": "eslint './src/{**/*.js, *.js}' --fix",
    "prettier": "prettier './src/{**/*.js, *.js}'",
    "prettier:fix": "prettier './src/{**/*.js, *.js}' --write"
  },
  "dependencies": {
    "prop-types": "^15.6.0",
    "react": "^16.7.0-alpha.2",
    "react-dom": "^16.7.0-alpha.2",
    "styled-components": "^4.1.0"
  },
  "devDependencies": {
    "@babel/core": "7.2.2",
    "@babel/plugin-proposal-class-properties": "7.2.3",
    "@babel/plugin-proposal-export-default-from": "7.2.0",
    "@babel/plugin-proposal-optional-chaining": "7.2.0",
    "@babel/preset-env": "7.2.3",
    "@babel/preset-react": "7.0.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "10.0.1",
    "babel-jest": "23.6.0",
    "babel-loader": "8.0.4",
    "babel-plugin-styled-components": "1.10.0",
    "eslint": "5.11.1",
    "eslint-config-react-app": "3.0.6",
    "eslint-plugin-babel": "5.3.0",
    "eslint-plugin-flowtype": "3.2.0",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-jsx-a11y": "6.1.2",
    "eslint-plugin-react": "7.12.2",
    "html-webpack-plugin": "3.2.0",
    "jest": "23.6.0",
    "jest-dom": "3.0.0",
    "prettier": "1.15.3",
    "react-testing-library": "5.4.2",
    "webpack": "4.28.3",
    "webpack-cli": "3.2.0",
    "webpack-dev-server": "3.1.14"
  },
  "peerDependencies": {
    "react": ">= 16.6.0",
    "react-dom": ">= 16.6.0",
    "styled-components": ">= 4.1.0"
  }
}
