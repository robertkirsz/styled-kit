{
  "name": "styled-kit",
  "version": "1.0.5",
  "description": "A React UI components kit powered by styled-components",
  "author": "Robert Kirsz <robert.kirsz@gmail.com>",
  "license": "MIT",
  "homepage": "http://robertkirsz.github.io/styled-kit",
  "repository": {
    "type": "git",
    "url": "https://github.com/robertkirsz/styled-kit.git"
  },
  "scripts": {
    "start": "NODE_ENV=dev webpack-dev-server --config webpack.config.dev.js --hot",
    "build": "NODE_ENV=prod webpack --config webpack.config.prod.js",
    "build-demo": "NODE_ENV=prod webpack --config webpack.config.dev.js",
    "test": "NODE_ENV=test jest --watch",
    "test:verbose": "NODE_ENV=test jest --verbose --watch",
    "test:coverage": "NODE_ENV=test jest --coverage",
    "lint": "eslint './src/{**/*.js,*.js}' --fix",
    "format": "prettier './src/{**/*.js,*.js}' --write --check",
    "predeploy": "npm run build-demo",
    "deploy": "gh-pages -d demo"
  },
  "devDependencies": {
    "@babel/core": "7.11.6",
    "@babel/plugin-proposal-class-properties": "7.10.4",
    "@babel/plugin-proposal-export-default-from": "7.10.4",
    "@babel/plugin-proposal-optional-chaining": "7.11.0",
    "@babel/plugin-transform-runtime": "7.11.5",
    "@babel/preset-env": "7.11.5",
    "@babel/preset-react": "7.10.4",
    "@babel/runtime": "7.11.2",
    "@typescript-eslint/eslint-plugin": "4.3.0",
    "@typescript-eslint/parser": "4.3.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "10.1.0",
    "babel-jest": "26.3.0",
    "babel-loader": "8.1.0",
    "babel-plugin-styled-components": "1.11.1",
    "eslint": "7.10.0",
    "eslint-config-react-app": "5.2.1",
    "eslint-plugin-babel": "5.3.1",
    "eslint-plugin-flowtype": "5.2.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsx-a11y": "6.3.1",
    "eslint-plugin-react": "7.21.2",
    "eslint-plugin-react-hooks": "4.1.2",
    "gh-pages": "3.1.0",
    "html-webpack-plugin": "4.5.0",
    "jest": "26.4.2",
    "jest-styled-components": "7.0.3",
    "prettier": "2.1.2",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-test-renderer": "16.13.1",
    "styled-components": "5.2.0",
    "typescript": "4.0.3",
    "webpack": "4.44.2",
    "webpack-cli": "3.3.12",
    "webpack-dev-server": "3.11.0"
  }
}
